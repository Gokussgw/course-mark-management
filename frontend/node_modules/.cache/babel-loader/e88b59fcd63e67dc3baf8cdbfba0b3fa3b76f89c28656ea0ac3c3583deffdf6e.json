link {\r\n  margin-top: auto;\r\n  text-align: center;\r\n  font-size: 14px;\r\n  color: #666;\r\n}\r\n\r\n.register-link a {\r\n  color: #0f4c81;\r\n  font-weight: 600;\r\n  text-decoration: none;\r\n  transition: all 0.2s;\r\n}\r\n\r\n.register-link a:hover {\r\n  text-decoration: underline;\r\n}\r\n\r\n/* Responsive design */\r\n@media (max-width: 768px) {\r\n  .login-container {\r\n    flex-direction: column;\r\n    max-width: 500px;\r\n  }\r\n  \r\n  .login-branding {\r\n    padding: 30px 20px;\r\n  }\r\n  \r\n  .login-form-container {\r\n    padding: 30px 20px;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":";;;;AA0IA,SAASA,UAAS,QAAS,MAAM;AAEjC,eAAe;EACbC,IAAI,EAAE,OAAO;EACbC,IAAI,WAAJA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,SAAS,EAAE,OAAO;MAClBC,UAAU,EAAE;QACVC,KAAK,EAAE,EAAE;QACTC,QAAQ,EAAE;MACZ,CAAC;MACDC,YAAY,EAAE;QACZC,YAAY,EAAE,EAAE;QAChBC,GAAG,EAAE;MACP,CAAC;MACDC,KAAK,EAAE,EAAE;MACTC,YAAY,EAAE,KAAK;MACnBC,UAAU,EAAE;IACd,CAAC;EACH,CAAC;EACDC,QAAQ,EAAAC,aAAA,KACHd,UAAU,CAAC,CAAC,WAAW,CAAC,EAC5B;EACDe,OAAO,EAAE;IACPC,YAAY,WAAZA,YAAYA,CAACC,GAAG,EAAE;MAChB,IAAI,CAACd,SAAQ,GAAIc,GAAG;MACpB,IAAI,CAACP,KAAI,GAAI,EAAE,EAAE;IACnB,CAAC;IAEDQ,cAAc,WAAdA,cAAcA,CAAA,EAAG;MACf,IAAI,CAACP,YAAW,GAAI,CAAC,IAAI,CAACA,YAAY;IACxC,CAAC;IAEKQ,gBAAgB,WAAhBA,gBAAgBA,CAAA,EAAG;MAAA,IAAAC,KAAA;MAAA,OAAAC,iBAAA,cAAAC,YAAA,GAAAC,CAAA,UAAAC,QAAA;QAAA,IAAAC,IAAA,EAAAC,EAAA;QAAA,OAAAJ,YAAA,GAAAK,CAAA,WAAAC,QAAA;UAAA,kBAAAA,QAAA,CAAAC,CAAA;YAAA;cACvBT,KAAI,CAACV,KAAI,GAAI,EAAE;cAAAkB,QAAA,CAAAE,CAAA;cAAAF,QAAA,CAAAC,CAAA;cAAA,OAGMT,KAAI,CAACW,MAAM,CAACC,QAAQ,CAAC,YAAY,EAAEZ,KAAI,CAAChB,UAAU,CAAC;YAAA;cAAhEqB,IAAG,GAAAG,QAAA,CAAAK,CAAA;cAET;cACA,IAAIR,IAAI,CAACS,IAAG,KAAM,UAAU,EAAE;gBAC5Bd,KAAI,CAACe,OAAO,CAACC,IAAI,CAAC,qBAAqB,CAAC;cAC1C,OAAO,IAAIX,IAAI,CAACS,IAAG,KAAM,SAAS,EAAE;gBAClCd,KAAI,CAACe,OAAO,CAACC,IAAI,CAAC,oBAAoB,CAAC;cACzC,OAAO,IAAIX,IAAI,CAACS,IAAG,KAAM,OAAO,EAAE;gBAChCd,KAAI,CAACe,OAAO,CAACC,IAAI,CAAC,kBAAkB,CAAC;cACvC,OAAO;gBACLhB,KAAI,CAACV,KAAI,GAAI,8BAA8B;cAC7C;cAAAkB,QAAA,CAAA