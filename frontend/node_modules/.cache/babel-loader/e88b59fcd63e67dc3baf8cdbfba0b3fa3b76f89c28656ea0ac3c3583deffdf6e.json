el=\"studentLogin.matricNumber\" \r\n                required\r\n                autocomplete=\"username\"\r\n              >\r\n            </div>\r\n            \r\n            <div class=\"input-group\">\r\n              <span class=\"input-icon\"><i class=\"fas fa-lock\"></i></span>\r\n              <input \r\n                :type=\"showPassword ? 'text' : 'password'\" \r\n                class=\"form-input\" \r\n                placeholder=\"PIN\" \r\n                v-model=\"studentLogin.pin\" \r\n                required\r\n                autocomplete=\"current-password\"\r\n              >\r\n              <span class=\"password-toggle\" @click=\"togglePassword\">\r\n                <i :class=\"showPassword ? 'fas fa-eye-slash' : 'fas fa-eye'\"></i>\r\n              </span>\r\n            </div>\r\n            \r\n            <div class=\"remember-forgot\">\r\n              <div class=\"remember-me\">\r\n                <input type=\"checkbox\" id=\"remember-student\" v-model=\"rememberMe\">\r\n                <label for=\"remember-student\">Remember me</label>\r\n              </div>\r\n              <a href=\"#\" class=\"forgot-password\">Forgot PIN?</a>\r\n            </div>\r\n            \r\n            <button \r\n              type=\"submit\" \r\n              class=\"login-button\"\r\n              :disabled=\"isLoading\"\r\n            >\r\n              <span v-if=\"isLoading\" class=\"spinner-border spinner-border-sm me-2\" role=\"status\" aria-hidden=\"true\"></span>\r\n              <span>Sign In</span>\r\n            </button>\r\n          </form>\r\n        </div>\r\n\r\n        <div class=\"register-link\">\r\n          <p>Don't have an account? <router-link to=\"/register\">Register</router-link></p>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapGetters } from 'vuex';\r\n\r\nexport default {\r\n  name: 'Login',\r\n  data() {\r\n    return {\r\n      activeTab: 'staff',\r\n      staffLogin: {\r\n        email: '',\r\n        password: ''\r\n      },\r\n      studentLogin: {\r\n        matricNumber: '',\r\n        pin: ''\r\n      },\r\n      error: '',\r\n      showPassword: false,\r\n      rememberMe: false\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapGetters(['isLoading'])\r\n  },\r\n  methods: {\r\n    setActiveTab(tab) {\r\n      this.activeTab = tab;\r\n      this.error = ''; // Clear errors when switching tabs\r\n    },\r\n\r\n    togglePassword() {\r\n      this.showPassword = !this.showPassword;\r\n    },\r\n    \r\n    async handleStaffLogin() {\r\n      this.error = '';\r\n      \r\n      try {\r\n        const user = await this.$store.dispatch('auth/login', this.staffLogin);\r\n        \r\n        // Redirect based on user role\r\n        if (user.role === 'lecturer') {\r\n          this.$router.push('/lecturer/dashboard');\r\n        } else if (user.role === 'advisor') {\r\n          this.$router.push('/advisor/dashboard');\r\n        } else if (user.role === 'admin') {\r\n          this.$router.push('/admin/dashboard');\r\n        } else {\r\n          this.error = 'Invalid role for staff login';\r\n        }\r\n      } catch (error) {\r\n        this.error = error.message || 'Invalid login credentials';\r\n      }\r\n    },\r\n    \r\n    async handleStudentLogin() {\r\n      this.error = '';\r\n      \r\n      try {\r\n        const user = await this.$store.dispatch('auth/studentLogin', this.studentLogin);\r\n        \r\n        if (user.role === 'student') {\r\n          this.$router.push('/student/dashboard');\r\n        } else {\r\n          this.error = 'Invalid student credentials';\r\n        }\r\n      } catch (error) {\r\n        this.error = error.message || 'Invalid matric number or PIN';\r\n      }\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.login-page {\r\n  display: flex;\r\n  min-height: 100vh;\r\n  align-items: center;\r\n  justify-content: center;\r\n  background: linear-gradient(135deg, #1e