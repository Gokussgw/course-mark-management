ments',\n          severity: 'danger'\n        }, {\n          description: 'Poor attendance (30%)',\n          severity: 'danger'\n        }]\n      }],\n      filters: {\n        highRisk: true,\n        mediumRisk: true,\n        lowRisk: true,\n        lowGrades: false,\n        missedAssessments: false,\n        attendance: false\n      },\n      searchQuery: '',\n      sortField: 'riskLevel',\n      sortDirection: 'asc',\n      selectedStudent: null,\n      newNote: '',\n      newNotesCount: 2\n    };\n  },\n  computed: _objectSpread(_objectSpread(_objectSpread({}, mapGetters(['isLoading'])), mapGetters('auth', ['getUser'])), {}, {\n    filteredStudents: function filteredStudents() {\n      var _this = this;\n      return this.students.filter(function (student) {\n        // Risk level filter\n        if (!_this.filters.highRisk && student.riskLevel === 'high') return false;\n        if (!_this.filters.mediumRisk && student.riskLevel === 'medium') return false;\n        if (!_this.filters.lowRisk && student.riskLevel === 'low') return false;\n\n        // Risk factors filter\n        if (_this.filters.lowGrades && !student.riskFactors.some(function (f) {\n          return f.description.includes('below') || f.description.includes('Failed');\n        })) {\n          return false;\n        }\n        if (_this.filters.missedAssessments && !student.riskFactors.some(function (f) {\n          return f.description.includes('missed') || f.description.includes('submission');\n        })) {\n          return false;\n        }\n        if (_this.filters.attendance && !student.riskFactors.some(function (f) {\n          return f.description.includes('attendance') || f.description.includes('classes');\n        })) {\n          return false;\n        }\n\n        // Search query\n        if (_this.searchQuery && !student.name.toLowerCase().includes(_this.searchQuery.toLowerCase()) && !student.matricNumber.toLowerCase().includes(_this.searchQuery.toLowerCase())) {\n          return false;\n        }\n        return true;\n      });\n    },\n    sortedStudents: function sortedStudents() {\n      var _this2 = this;\n      return _toConsumableArray(this.filteredStudents).sort(function (a, b) {\n        var comparison = 0;\n        if (_this2.sortField === 'name') {\n          comparison = a.name.localeCompare(b.name);\n        } else if (_this2.sortField === 'average') {\n          comparison = a.average - b.average;\n        } else if (_this2.sortField === 'riskLevel') {\n          var riskValue = {\n            'high': 3,\n            'medium': 2,\n            'low': 1\n          };\n          comparison = riskValue[a.riskLevel] - riskValue[b.riskLevel];\n        }\n        return _this2.sortDirection === 'asc' ? comparison : -comparison;\n      });\n    },\n    riskCounts: function riskCounts() {\n      var counts = {\n        high: 0,\n        medium: 0,\n        low: 0\n      };\n      this.students.forEach(function (student) {\n        counts[student.riskLevel]++;\n      });\n      return counts;\n    }\n  }),\n  methods: {\n    sortBy: function sortBy(field) {\n      if (this.sortField === field) {\n        this.sortDirection = this.sortDirection === 'asc' ? 'desc' : 'asc';\n      } else {\n        this.sortField = field;\n        this.sortDirection = 'asc';\n      }\n    },\n    getSortIconClass: function getSortIconClass(field) {\n      if (this.sortField !== field) return 'fa-sort';\n      return this.sortDirection === 'asc' ? 'fa-sort-up' : 'fa-sort-down';\n    },\n    getRiskBadgeClass: function getRiskBadgeClass(level) {\n      if (level === 'high') return 'bg-danger';\n      if (level === 'medium') return 'bg-warning';\n      return 'bg-success';\n    },\n    resetFilters: function resetFilters() {\n      this.filters = {\n        highRisk: true,\n        mediumRisk: true,\n        lowRisk: true,\n        lowGrades: false,\n        missedAssessments: false,\n        attendance: false\n      };\n      this.searchQuery = '';\n    },\n    openAddNoteModal: function openAddNoteModal(student) {\n      this.selectedStudent = student;\n      this.newNote = '';\n\n      // Open modal\n      var modal = new bootstrap.Modal(document.getElementById('addNoteModal'));\n      modal.show();\n    },\n    saveNote: function saveNote() {\n      var _this3 = this;\n      return _asyncToGenerator(/*#__PURE__*/_regenerator().m(function _callee() {\n        var modalElement, modal;\n        return _regenerator().w(function (_context) {\n          while (1) switch (_context.n) {\n            case 0:\n              if (!(!_this3.newNote.trim() || !_this3.selectedStudent)) {\n                _context.n = 1;\n                break;\n              }\n              return _context.a(2);\n            case 1:\n              try {\n                // In a real app, this would be an API call\n                console.log(\"Saving note for student \".concat(_this3.selectedStudent.id, \": \").concat(_this3.newNote));\n\n                // Close modal\n                modalElement = document.getElementById('addNoteModal');\n                modal = bootstrap.Modal.getInstance(modalElement);\n                modal.hide();\n\n                // Show success message\n                alert('Note saved successfully');\n\n                // Update notes count (in a real app this would come from the API)\n                _this3.newNotesCount++;\n              } catch (error) {\n                console.error('Error saving note:', error);\n                alert('Failed to save note. Please try again.');\n              }\n            case 2:\n              return _context.a(2);\n          }\n        }, _callee);\n      }))();\n    },\n    contactStudent: function contactStudent(student) {\n      // In a real app, this might open an email client or messaging system\n      alert(\"Contact feature for \".concat(student.name, \" would open here\"));\n    },\n    exportToCSV: function exportToCSV() {\n      // Convert students data to C